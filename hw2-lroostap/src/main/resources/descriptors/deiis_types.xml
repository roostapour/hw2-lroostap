<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>deiis_types</name>
  <description/>
  <version>1.0</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>edu.cmu.deiis.types.Annotation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>casProcessorId</name>
          <description>The unique identifier of the CAS processor that added this annotation to the CAS</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description>The confidence level assigned to the Annotation by the CAS processor that created it.</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Token</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.NGram</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>elements</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.Annotation</elementType>
        </featureDescription>
        <featureDescription>
          <name>elementType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>N</name>
          <description>This is the size N in Ngram. For example 1 for unigram, 2 for bigram.</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>Owner</name>
          <description>It shows that each ngram is related to which question or answer.</description>
          <rangeTypeName>edu.cmu.deiis.types.Annotation</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Answer</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Sentence</supertypeName>
      <features>
        <featureDescription>
          <name>isCorrect</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Question</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Sentence</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.AnswerScore</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>score</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answer</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.Answer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.types.Sentence</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>Tokens</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.Token</elementType>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
